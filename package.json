{
  "name": "jest-websocket-mock",
  "version": "2.4.0",
  "description": "Mock websockets and assert complex websocket interactions with Jest",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romgain/jest-websocket-mock.git"
  },
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:lib && npm run build:types",
    "build:lib": "vite build",
    "build:types": "tsc -p tsconfig.build.json",
    "prettier:check": "prettier --list-different \"src/**/*.{ts,js,md}\" \"**/*.md\"",
    "prettier:apply": "prettier --write \"src/**/*.{ts,js}\" \"**/*.md\"",
    "type:check": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "keywords": [
    "jest",
    "websocket",
    "mock",
    "unit-testing"
  ],
  "author": "Akiomi Kamakura",
  "license": "MIT",
  "devDependencies": {
    "prettier": "^2.0.2",
    "rimraf": "^4.1.2",
    "typescript": "^4.0.2",
    "vite": "^4.3.9",
    "vite-plugin-node-stdlib-browser": "^0.2.1",
    "vitest": "^0.25.3"
  },
  "dependencies": {
    "jest-diff": "^29.2.0",
    "mock-socket": "^9.1.0"
  },
  "files": [
    "dist",
    "LICENSE"
  ]
}
